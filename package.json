{
	"name": "diet-app",
	"version": "0.0.0",
	"author": "Oluwasetemi Ojo <setemiojo@gmail.com> (https://oluwasetemi.dev/)",
	"license": "MIT",
	"keywords": [],
	"main": "main.tsx",
	"description": "TalentQL pipeline program group project",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"lint:eslint": "eslint . --ext ts,tsx,js",
		"lint:prettier": "prettier --write \"**/*.{js,json,ts,tsx}\"",
		"lint:types": "tsc",
		"lint:css": "stylelint './src/**/*.(ts|tsx)'",
		"lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:types && yarn lint:css",
		"validate": "yarn lint && yarn test:unit",
		"test:coverage": "yarn test:unit --coverage",
		"test:watch": "yarn test:unit --watch",
		"test:unit": "jest --passWithNoTests",
		"test:all": "run-s test test:e2e",
		"test": "is-ci \"test:coverage\" \"test:watch\" ",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
		"test:e2e:run": "run-p -r dev cy:run",
		"test:e2e:dev": "npm-run-all --parallel dev cy:open",
		"test:e2e:ci": "cypress run --record --key 99a444d8-885e-419f-a6da-d8d103be22a6",
		"prepare": "husky install",
		"lintstaged": "lint-staged"
	},
	"dependencies": {
		"@atlaskit/avatar": "^20.5.4",
		"@atlaskit/button": "^16.1.2",
		"@atlaskit/checkbox": "^12.3.3",
		"@atlaskit/form": "^8.4.2",
		"@atlaskit/tabs": "^13.2.6",
		"@atlaskit/textfield": "^5.1.2",
		"@emotion/react": "^11.5.0",
		"@emotion/styled": "^11.3.0",
		"@reduxjs/toolkit": "^1.6.0",
		"@types/react-bootstrap": "^0.32.26",
		"bootstrap": "^5.1.0",
		"bootstrap-reboot": "^4.5.6",
		"classnames": "^2.3.1",
		"react": "^17.0.0",
		"react-bootstrap": "^2.0.0-beta.6",
		"react-dom": "^17.0.0",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-select": "^4.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@testing-library/cypress": "^7.0.6",
		"@testing-library/dom": "^8.0.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.1.9",
		"@types/jest": "^26.0.23",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.28.0",
		"@typescript-eslint/parser": "^4.28.0",
		"@vitejs/plugin-react-refresh": "^1.3.1",
		"babel-jest": "^27.0.6",
		"cypress": "^8.3.1",
		"eslint": "^7.29.0",
		"eslint-plugin-cypress": "^2.11.3",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jest": "^24.3.6",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-prettier-config": "^1.0.1",
		"husky": "^7.0.1",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^3.0.0",
		"is-ci-cli": "^2.2.0",
		"jest": "^27.0.6",
		"jest-watch-typeahead": "^0.6.4",
		"lint-staged": "^11.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.2",
		"sass": "^1.42.0",
		"start-server-and-test": "^1.12.6",
		"stylelint": "^13.13.1",
		"stylelint-config-recommended": "^5.0.0",
		"typescript": "^4.3",
		"vite": "^2.5.6",
		"vite-plugin-react-svg": "^0.2.0",
		"waait": "^1.0.5"
	},
	"prettier": {
		"singleQuote": true,
		"semi": true,
		"trailingComma": "all",
		"useTabs": true,
		"arrowParens": "always"
	}
}
